## -*- perl -*-
## ----------------------------------------------------------------------
## Locale/sk_SK.ISO8859-2/TextOV: locale for Slovak output in overstrike text format
## ----------------------------------------------------------------------

## ----------------------------------------------------------------------
%locale = (
	   'abstract' => 'Zhrnutie',
	   'copyright notice' => 'Autorské práva',
	   'contents' => 'Obsah',
	   'chapter' => sub { return "Kapitola $_[0]" },
	   'appendix' => sub { return "Dodatok $_[0]" },
	   'section' => sub { return "Oddiel $_[0]" },
	   'subsection' => sub { return "Oddiel $_[0]" },
	   'subsubsection' => sub { return "Oddiel $_[0]" },
	   'paragraph' => sub { return "Oddiel $_[0]" },
	   'subparagraph' => sub { return "Oddiel $_[0]" },
	   );

## ----------------------------------------------------------------------
