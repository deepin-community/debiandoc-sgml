## -*- perl -*-
## ----------------------------------------------------------------------
## Locale/ko_KR.UTF-8/HTML: locale for Korean output in HTML format
## ----------------------------------------------------------------------

## ----------------------------------------------------------------------
%locale = (
	   'charset' => 'utf-8',
	   'abstract' => '요약',
	   'copyright notice' => '저작권',
	   'contents' => '차례',
	   'chapter' => sub { return " $_[0]" },
	   'appendix' => sub { return "부록 $_[0]" },
	   'section' => sub { return " $_[0]" },
	   'subsection' => sub { return " $_[0]" },
	   'subsubsection' => sub { return " $_[0]" },
	   'paragraph' => sub { return " $_[0]" },
	   'subparagraph' => sub { return " $_[0]" },
	   'footnotes' => '각주',
	   'comments' => '주',
	   'next' => '다음',
	   'previous' => '이전',
	   );

