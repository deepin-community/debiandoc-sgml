## -*- perl -*-
## ----------------------------------------------------------------------
## Locale/de_DE.ISO8859-1/HTML: locale for German output in HTML format
## ----------------------------------------------------------------------

## ----------------------------------------------------------------------
%locale = (
	   'charset' => 'iso-8859-1',
	   'abstract' => 'Zusammenfassung',
	   'copyright notice' => 'Copyright-Hinweis',
	   'contents' => 'Inhalt',
	   'chapter' => sub { return "Kapitel $_[0]" },
	   'appendix' => sub { return "Anhang $_[0]" },
	   'section' => sub { return "Abschnitt $_[0]" },
	   'subsection' => sub { return "Abschnitt $_[0]" },
	   'subsubsection' => sub { return "Abschnitt $_[0]" },
	   'paragraph' => sub { return "Abschnitt $_[0]" },
	   'subparagraph' => sub { return "Abschnitt $_[0]" },
	   'footnotes' => 'Fußnoten',
	   'comments' => 'Comments',
	   'next' => 'weiter',
	   'previous' => 'zurück',
	   );

## ----------------------------------------------------------------------
