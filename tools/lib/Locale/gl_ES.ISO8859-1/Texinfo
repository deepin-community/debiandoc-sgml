## -*- perl -*-
## ----------------------------------------------------------------------
## Locale/gl_ES.ISO8859-1/Texinfo: locale for Galician output in Texinfo format
## ----------------------------------------------------------------------

## ----------------------------------------------------------------------
%locale = (
	   'detailed' => 'A Lista Detallada de Nós',
	   'chapter' => sub { return "Capítulo $_[0]" },
	   'appendix' => sub { return "Apéndice $_[0]" },
	   'section' => sub { return "Sección $_[0]" },
	   'subsection' => sub { return "Sección $_[0]" },
	   'subsubsection' => sub { return "Sección $_[0]" },
	   'paragraph' => sub { return "Sección $_[0]" },
	   'subparagraph' => sub { return "Sección $_[0]" }
	   );

## ----------------------------------------------------------------------
