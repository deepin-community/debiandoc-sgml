## -*- perl -*-
## ----------------------------------------------------------------------
## Locale/pl_PL.UTF-8/Texinfo: locale for Polish output in Texinfo format
## ----------------------------------------------------------------------

## ----------------------------------------------------------------------
%locale = (
	   'detailed' => 'Szczegółowa lista węzłów',
	   'chapter' => sub { return "Część $_[0]" },
	   'appendix' => sub { return "Dodatek $_[0]" },
	   'section' => sub { return "Rozdział $_[0]" },
	   'subsection' => sub { return "Rozdział $_[0]" },
	   'subsubsection' => sub { return "Rozdział $_[0]" },
	   'paragraph' => sub { return "Rozdział $_[0]" },
	   'subparagraph' => sub { return "Rozdział $_[0]" },
	   );

## ----------------------------------------------------------------------
