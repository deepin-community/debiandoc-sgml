## ----------------------------------------------------------------------
%locale = (
	   'detailed' => 'Yksityiskohtainen solmulista',
	   'chapter' =>sub { return " Luku $_[0]" },
	   'appendix' => sub { return "Liite $_[0]" },
	   'section' => sub { return "Kohta $_[0]" },
	   'subsection' => sub { return "Kohta $_[0]" },
	   'subsubsection' => sub { return "Kohta $_[0]" },
	   'paragraph' => sub { return "Kohta $_[0]" },
	   'subparagraph' => sub { return "Kohta $_[0]" },
	   );

