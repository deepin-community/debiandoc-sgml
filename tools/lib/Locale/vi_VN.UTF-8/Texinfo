## -*- perl -*-
## ----------------------------------------------------------------------
## Locale/vi_VN.UTF-8/Texinfo: locale for Vietnamese output in Texinfo format
## ----------------------------------------------------------------------

## ----------------------------------------------------------------------
%locale = (
	   'detailed' => 'Danh Sách Nút Chi Tiết',
	   'chapter' => sub { return "Chương $_[0]" },
	   'appendix' => sub { return "Phụ lục $_[0]" },
	   'section' => sub { return "Phần $_[0]" },
	   'subsection' => sub { return "Phần phụ $_[0]" },
	   'subsubsection' => sub { return "Phần phụ con $_[0]" },
	   'paragraph' => sub { return "Đoạn văn $_[0]" },
	   'subparagraph' => sub { return "Đoạn văn phụ $_[0]" },
	   );

## ----------------------------------------------------------------------
