## ----------------------------------------------------------------------
%locale = (
	   'abstract' => 'Santrauka',
	   'copyright notice' => 'AutorinÄ—s teisÄ—s',
	   'contents' => 'Turinys',
	   'chapter' => sub { return "$_[0] skyrius" },
	   'appendix' => sub { return "Priedas $_[0]" },
	   'section' => sub { return "$_[0] skyrius" },
	   'subsection' => sub { return "$_[0] skyrius" },
	   'subsubsection' => sub { return "$_[0] skyrius" },
	   'paragraph' => sub { return "$_[0] skyrius" },
	   'subparagraph' => sub { return "$_[0] skyrius" },
	   );

